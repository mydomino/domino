= cache('navbar') do
  .container.row.navbar.navbar-default.navbar-fixed-top
    = link_to embedded_svg('penguin_horizontal.svg'), root_path
    .navbar-header.pull-right
      %ul.nav.navbar-nav
        %li
          %p= link_to 'About', about_path
        %li
          %p= link_to 'Press', 'http://mydomino.strikingly.com', target: '_blank'
        %li
          %p= link_to 'Blog', 'http://www.beyondfossilfuels.org', target: '_blank'
        %li.button
          .small= render 'common/getstarted_button'
